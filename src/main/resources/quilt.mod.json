{
  "schema_version": 1,
  "quilt_loader": {
    "group": "me.codesquid",
    "id": "touchup",
    "version": "${version}",
    "metadata": {
      "name": "Touch-up",
      "description": "Minor tweaks to Minecraft's visuals.",
      "contributors": {
        "CodeSquid": "Owner"
      },
      "contact": {
        "sources": "https://github.com/TheCodeSquid/touchup",
        "issues": "https://github.com/TheCodeSquid/touchup/issues"
      },
      "icon": "assets/touchup/icon.png",
      "license": "MIT"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "client_init": "me.codesquid.touchup.TouchUp"
    },
    "depends": [
      {
        "id": "quilt_loader",
        "versions": ">=0.21.2"
      },
      {
        "id": "quilted_fabric_api",
        "versions": ">=7.4.0"
      },
      {
        "id": "minecraft",
        "versions": ">=1.20"
      }
    ]
  },
  "mixin": "touchup.mixins.json",
  "minecraft": {
    "environment": "client"
  }
}